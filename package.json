{
  "name": "@rio-cloud/vite-rio-template",
  "description": "The RIO Vite Frontend template.",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "react-app-rewired test",
    "pretest": "tsc",
    "license-check": "license-checker --production --json --onlyAllow \"`cat whitelist.txt`\" --excludePackages \"rio-startertemplate-web@0.1.0\" --out licenses.txt",
    "lint": "eslint src",
    "lint-fix": "npm run lint -- --fix",
    "audit": "npm audit --production --registry https://registry.npmjs.org --parseable",
    "dev-mock-server": "nodenv mocks/rio-starter-devmocks.js --exec nodemon",
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "format-code": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "pull-translations": "phrase pull --access_token $PHRASEAPP_ACCESS_TOKEN && npm run merge-translations",
    "push-translations": "phrase push --access_token $PHRASEAPP_ACCESS_TOKEN",
    "merge-translations": "nodenv ./mergeTranslations.js",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "cypress-ci": "start-server-and-test start http://localhost:3000 cypress-run"
  },
  "dependencies": {
    "@redux-devtools/extension": "^3.2.2",
    "@reduxjs/toolkit": "^1.8.3",
    "@rio-cloud/rio-session-expired-info": "0.9.10",
    "@rio-cloud/rio-uikit": "^0.16.1-beta.11",
    "@rio-cloud/rio-user-menu-component": "^2.0.0-beta-1",
    "@sentry/browser": "7.7.0",
    "iframe-resizer-react": "^1.1.0",
    "lodash": "^4.17.21",
    "oidc-client-ts": "^2.0.5",
    "qs": "6.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.23.0",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-dynamic-import-vars": "^1.4.3",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.176",
    "@types/node": "^14.17.32",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.20",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-react": "^2.0.0",
    "cypress": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "http-proxy-middleware": "^1.3.1",
    "husky": "^7.0.0",
    "jest-changed-files": "^26.6.2",
    "jest-environment-jsdom": "^26.6.2",
    "jest-junit": "^12.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.0.0",
    "node-env-run": "^4.0.2",
    "nodemon": "^2.0.14",
    "prettier": "^2.5.0",
    "source-map-explorer": "^2.5.2",
    "start-server-and-test": "^1.12.1",
    "style-loader": "^1.3.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dynamic-import": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 2 versions",
    "last 5 Chrome versions",
    "Firefox >= 60",
    "Edge >= 15",
    "Safari >= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "ReactDOM": "react-dom"
    },
    "moduleDirectories": [
      "node_modules",
      "mocks"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./results/jest"
  }
}
