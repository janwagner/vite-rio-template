{
    "name": "@rio-cloud/vite-rio-template",
    "description": "The RIO Vite Frontend template.",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:ci": "vitest run --reporter=junit --outputFile.junit=./results/junit.xml",
        "test-dev": "vitest",
        "coverage": "vitest run --coverage",
        "license-check": "license-checker --production --json --onlyAllow \"`cat whitelist.txt`\" --excludePackages \"@rio-cloud/vite-rio-template@0.1.0\" --out licenses.txt",
        "lint": "eslint src",
        "lint-fix": "npm run lint -- --fix",
        "audit": "npm audit --production --registry https://registry.npmjs.org --parseable",
        "format-code": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format-code-on-commit": "npm run format-code && git add",
        "pull-translations": "phrase pull --access_token $PHRASEAPP_ACCESS_TOKEN && npm run merge-translations",
        "merge-translations": "nodenv ./mergeTranslations.js",
        "cypress-open": "cypress open",
        "cypress-run": "cypress run",
        "cypress-ci": "start-server-and-test start http://localhost:3000 cypress-run"
    },
    "dependencies": {
        "@redux-devtools/extension": "3.2.3",
        "@reduxjs/toolkit": "1.9.0",
        "@rio-cloud/rio-notifications-component": "0.2.2",
        "@rio-cloud/rio-session-expired-info": "0.9.13",
        "@rio-cloud/rio-uikit": "0.16.1",
        "@rio-cloud/rio-user-menu-component": "2.0.1",
        "@sentry/browser": "7.20.1",
        "events": "3.3.0",
        "iframe-resizer-react": "1.1.0",
        "lodash": "4.17.21",
        "oidc-client-ts": "2.1.0",
        "qs": "6.11.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.39.5",
        "react-intl": "6.2.1",
        "react-redux": "8.0.5",
        "react-router": "6.4.3",
        "react-router-dom": "6.4.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/lodash": "4.14.189",
        "@types/node": "18.11.9",
        "@types/qs": "6.9.7",
        "@types/react": "18.0.25",
        "@types/react-dom": "18.0.9",
        "@types/react-redux": "7.1.24",
        "@types/react-router": "5.1.19",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "5.43.0",
        "@typescript-eslint/parser": "5.43.0",
        "@vitejs/plugin-react": "2.2.0",
        "cypress": "11.1.0",
        "eslint": "8.28.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-compat": "4.0.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jest": "27.1.5",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "5.9.1",
        "license-checker": "25.0.1",
        "msw": "0.49.0",
        "nodenv": "0.1.0",
        "pre-commit": "1.2.2",
        "prettier": "2.7.1",
        "rollup-plugin-visualizer": "5.8.3",
        "start-server-and-test": "1.14.0",
        "typescript": "4.9.3",
        "vite": "3.2.4",
        "vitest": "0.25.2",
        "vitest-fetch-mock": "0.2.1"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        "last 2 versions",
        "last 5 Chrome versions",
        "Firefox >= 90",
        "Edge >= 95",
        "Safari >= 14"
    ],
    "pre-commit": [
        "format-code-on-commit"
    ],
    "msw": {
        "workerDirectory": ""
    }
}
